 xor     eax, 31
 add     ecx, 32
 test    esi, esi
 cmovne  ecx, eax
 bsr     eax, edi
 cmovne  ebp, eax
 bsr     eax, ebx
 xor     ebp, 31
 xor     eax, 31
 add     ebp, 32
 test    ebx, ebx
 cmove   eax, ebp
 mov     ebp, edx
 mov     edx, esi
 add     eax, 64
 or      ebp, esi
 mov     ebp, edi
 mov     esi, ebx
 cmovne  eax, ecx
 mov     ch, al
 and     ch, 127
 mov     cl, ch
 add     cl, -64
 shl     ebp, cl
 shld    esi, edi, cl
 test    cl, 32
 mov     cl, al
 cmovne  esi, ebp
 mov     ebp, edx
 mov     edx, dword, ptr, [esp, +, 32]
 shld    ebp, edx, cl
 shl     edx, cl
 test    al, 32
 mov     cl, ch
 cmove   edx, ebp
 sub     cl, 64
 mov     ebp, ebx
 xor     edi, edi
 neg     cl
