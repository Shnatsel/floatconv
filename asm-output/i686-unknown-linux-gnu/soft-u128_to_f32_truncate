 push    ebx
 push    edi
 push    esi
 mov     edi, dword, ptr, [esp, +, 32]
 bsr     ebp, dword, ptr, [esp, +, 28]
 mov     esi, dword, ptr, [esp, +, 24]
 mov     eax, 63
 bsr     ecx, edi
 xor     ebp, 31
 mov     ebx, edi
 xor     ecx, 31
 add     ebp, 32
 test    edi, edi
 cmovne  ebp, ecx
 bsr     ecx, dword, ptr, [esp, +, 20]
 cmovne  eax, ecx
 bsr     edx, esi
 xor     eax, 31
 xor     edx, 31
 add     eax, 32
 test    esi, esi
 cmove   edx, eax
 mov     eax, dword, ptr, [esp, +, 28]
 add     edx, 64
 or      eax, edi
 mov     eax, dword, ptr, [esp, +, 20]
 mov     edi, esi
 cmovne  edx, ebp
 mov     ch, dl
 and     ch, 127
 mov     cl, ch
 mov     ebp, eax
 mov     esi, eax
 add     cl, -64
 shld    edi, eax, cl
 mov     eax, dword, ptr, [esp, +, 28]
 shl     ebp, cl
 test    cl, 32
 mov     cl, dl
 cmovne  edi, ebp
 mov     ebp, dword, ptr, [esp, +, 24]
 shld    ebx, eax, cl
 shl     eax, cl
 test    dl, 32
 mov     cl, ch
 cmove   eax, ebx
 sub     cl, 64
 mov     ebx, 0
 neg     cl
 shr     ebp, cl
 test    cl, 32
 cmovne  ebp, ebx
 xor     ebx, ebx
 or      ebp, eax
 mov     eax, dword, ptr, [esp, +, 32]
 cmp     ch, 64
 cmovae  ebp, edi
 test    ch, ch
 mov     ecx, dword, ptr, [esp, +, 24]
 cmove   ebp, eax
 or      esi, dword, ptr, [esp, +, 28]
 shl     edx, 23
 or      ecx, eax
 mov     eax, 1585446912
 shr     ebp, 8
 sub     eax, edx
 or      esi, ecx
 cmove   eax, ebx
 add     eax, ebp
 pop     esi
 pop     edi
 pop     ebx
 pop     ebp
 ret
