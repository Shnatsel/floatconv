floatconv::fast::u64_to_f32_truncate:
 clz     r2, r0
 cmp     r1, #0
 add     r12, r2, #32
 clzne   r12, r1
 and     r3, r12, #63
 rsb     r2, r3, #32
 lsr     r2, r0, r2
 orr     r2, r2, r1, lsl, r3
 subs    r3, r3, #32
 lslpl   r2, r0, r3
 orrs    r0, r0, r1
 lsr     r3, r2, #8
 sub     r3, r3, r12, lsl, #23
 add     r3, r3, #511705088
 add     r3, r3, #1073741824
 lsreq   r3, r2, #8
 vmov    s0, r3
 bx      lr
