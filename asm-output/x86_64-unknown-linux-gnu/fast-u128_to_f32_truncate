floatconv::fast::u128_to_f32_truncate:
 bsr     rax, rdi
 mov     edx, 127
 cmovne  rdx, rax
 xor     rdx, 63
 add     rdx, 64
 bsr     rcx, rsi
 xor     rcx, 63
 test    rsi, rsi
 cmove   rcx, rdx
 mov     rax, rsi
 shld    rax, rdi, cl
 mov     rdx, rdi
 shl     rdx, cl
 test    cl, 64
 cmove   rdx, rax
 shr     rdx, 40
 shl     ecx, 23
 mov     eax, 2122317824
 sub     eax, ecx
 xor     ecx, ecx
 or      rdi, rsi
 cmovne  ecx, eax
 add     ecx, edx
 movd    xmm0, ecx
 ret
