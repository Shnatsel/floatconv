floatconv::u32_to_f64:
 cbz     r0, .LBB0_2
 clz     r1, r0
 rsb.w   r3, r1, #11
 subs.w  r2, r1, #11
 lsr.w   r3, r0, r3
 it      pl
 lslpl.w r3, r0, r2
 cmp     r2, #0
 sub.w   r3, r3, r1, lsl, #20
 add.w   r1, r1, #21
 add.w   r3, r3, #1073741824
 add.w   r3, r3, #30408704
 lsl.w   r0, r0, r1
 it      pl
 movpl   r0, #0
 vmov    d0, r0, r3
 bx      lr
.LBB0_2:
 vmov.i32 d0, #0x0
 bx      lr
