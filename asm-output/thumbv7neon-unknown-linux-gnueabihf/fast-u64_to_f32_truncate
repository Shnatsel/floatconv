floatconv::fast::u64_to_f32_truncate:
 push    {r7, lr}
 clz     r2, r0
 cmp     r1, #0
 add.w   lr, r2, #32
 it      ne
 clzne   lr, r1
 and     r3, lr, #63
 rsb.w   r2, r3, #32
 lsl.w   r12, r1, r3
 subs    r3, #32
 lsr.w   r2, r0, r2
 orr.w   r2, r2, r12
 it      pl
 lslpl.w r2, r0, r3
 lsrs    r3, r2, #8
 sub.w   r3, r3, lr, lsl, #23
 orrs    r0, r1
 add.w   r3, r3, #1585446912
 it      eq
 lsreq   r3, r2, #8
 vmov    s0, r3
 pop     {r7, pc}
